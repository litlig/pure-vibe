{
    "manifest_version": 3,
    "name": "Puppy Rain",
    "version": "1.1",
    "description": "Press Shift + CMD + p to make puppies rain on any page!",
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "css": ["style.css"]
      }
    ],
    "web_accessible_resources": [
        {
          "resources": ["puppies/*.png"],
          "matches": ["<all_urls>"]
        }   
    ],
    "icons": {
      "128": "puppies/1.png"
    }
  }